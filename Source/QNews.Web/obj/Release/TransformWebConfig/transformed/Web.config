          <action type="Redirect" url="/gioi-thieu-co-cau-to-chuc-bo-may-chuong-trinh" redirectType="Permanent" />
        </rule>
        <rule name="Redirect www" patternSyntax="Wildcard" stopProcessing="true">
          <match url="*" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="www.vpctqg.gov.vn" />
          </conditions>
          <action type="Redirect" url="http://vpctqg.gov.vn/{R:0}" redirectType="Permanent" />
        </rule>
      </rules>
      <outboundRules></outboundRules>
    </rewrite>
