@model QNews.Base.Clone_Domain

@{
    ViewBag.Title = "Xem chi tiết";
    Layout = "~/Views/Shared/_Ajax.cshtml";
}

<fieldset class="forms">
    <table class="tableforms tabledetails" cellspacing="1" width="100%">
        <tr>
            <td style="width:90px;" class="label"><label>Tên domain</label></td>
            <td>
                @Html.DisplayFor(model => model.Title)
            </td>
        </tr>

        <tr>
            <td style="width:90px;" class="label"><label>Địa chỉ domain</label></td>
            <td>
                @Html.DisplayFor(model => model.DomainUrl)
            </td>
        </tr>

        <tr>
            <td class="label"><label>Mã Xpath Tiêu đề</label></td>
            <td>@Model.XpathTitle</td>
        </tr>
        
        <tr>
            <td class="label"><label>Mô tả</label></td>
            <td>
                @Model.XpathTitle
            </td>
        </tr>

    </table>
</fieldset>
